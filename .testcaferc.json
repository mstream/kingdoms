{
    "assertionTimeout": 8000,
    "browsers": [
        "chrome:headless",
        "firefox:headless",
    ],
    "color": true,
    "concurrency": 4,
    "filter": {
        "testMeta": {}
    },
    "pageLoadTimeout": 8000,
    "quarantineMode": true,
    "reporter": [
        {
            "name": "custom"
