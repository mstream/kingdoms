{
    "name": "kingdoms-client",
    "version": "1.0.0",
    "private": true,
    "devDependencies": {
        "@babel/core": "7.8.7",
        "rimraf": "3.0.2",
        "webpack": "^4.42.0",
        "webpack-cli": "3.3.11",
        "webpack-dev-server": "3.10.3"
    },
    "scripts": {
        "build": "webpack --mode production",
        "predeploy-dev": "npm run build",
        "deploy-dev": "aws s3 cp --recursive --acl public-read dist/ s3://www.dev.kingdoms.maciej-laciak.com/",
        "predeploy-prod": "npm run build",
        "deploy-prod": "aws s3 cp --recursive --acl public-read dist/ s3://www.prod.kingdoms.maciej-laciak.com/",
        "prebuild": "rimraf dist",
        "start": "webpack-dev-server --open --mode development",
        "test": "jest --coverage"
    }
}
