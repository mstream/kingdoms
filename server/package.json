        "prebuild": "rimraf .aws-sam .vscode",
        "test": "jest --coverage"
    },
    "devDependencies": {
        "rimraf": "3.0.2",
        "webpack": "4.41.6",
        "webpack-cli": "3.3.11"
    }
}
